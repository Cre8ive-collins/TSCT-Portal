{
  "name": "TSCT Portal",
  "description": "The unofficial learning management system for Thaddeus Stevens College of Technology",
  "keywords": ["python", "flask", "postgresql", "heroku"],
  "addons": [{
    "plan": "heroku-postgresql",
    "options": {
      "version": "12"
    }
  }],
  "formation": {
    "web": {
      "quantity": 1,
      "size": "free"
    }
  },
  "env": {
    "FLASK_APP": {
      "value": "portal"
    }
  }
}
